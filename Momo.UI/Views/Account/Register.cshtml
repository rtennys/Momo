@model RegisterModel

@{
    ViewBag.Title = "Register";
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-group">@Html.EditorFor(m => m.Username)</div>
    <div class="form-group">@Html.EditorFor(m => m.Password)</div>
    <div class="form-group">@Html.EditorFor(m => m.ConfirmPassword)</div>

    <button type="submit" class="btn btn-secondary">Register</button>

    @Html.ValidationSummary()
}
